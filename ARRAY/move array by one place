include <bits/stdc++.h>
using namespace std ; 
int main(){
  int n ; 
  cout <<"enter the size of an array"<<endl; 
  cin>>n;
  int arr[n]; 
  for (int i = 0 ; i <n; i ++)
    cin>>arr[i]; 
  vector <int >temp; 
  for (int i = 0 ; i <n; i++){
    if(i ==0)
      temp.push_back(arr[n-1]); 
      else
      temp.push_back(arr[i-1]); 
  }
  for(int i = 0 ; i<n; i++)
cout <<temp[i]; 
  }
  
