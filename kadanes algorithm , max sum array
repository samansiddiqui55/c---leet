#include<iostream>
#include<map>
#include<vector>
using namespace std ; 

void major(vector<int>&nums){
 int  maxi = 0;  
    for (int i = 0 ; i <nums.size(); i ++){
        int sum = 0 ; 
        for ( int j = i ; j <nums.size();j ++){
                sum = sum+nums[j]; 
                maxi = max(sum, maxi); 
            }
    }
    cout <<maxi; 
}
int main(){
vector<int>nums = { 2,3,4}; 
    major(nums); 
}
   // better 
