#include <iostream>
using namespace std;

int main(){
    cout <<"enter a string"; 
    string s ; 
    cin>>s; // taking a string 
 int hash[26] = {0}; 
    for (int i = 0 ; i <s.size(); i ++){
        hash[s[i] -'a']++; 
    }

    int q =0; 
    cin>>q; 
    while (q--){
        cout <<"enter your query"<<endl;  
        char c ; 
        cin>>c; 
        
        cout <<hash[c-'a']; 
    }
    return 0;
}
