#include <iostream>
using namespace std ; 
int main() {
int arr[5] = {1,1,2,3,3,}; 
  int hash [5]= {0}; 
  for (int i = 0;i <5; i++){
    hash[arr[i]]++ ; 
  }
  for (int i = 0 ; i <5; i ++){
    if(hash[i]>1){
      cout <<"appears more than once"<<i<<endl; 
    }
    else if(hash[i]==1){
      cout <<"does not appear"<<i<<endl; 
    }
  }
  


}
