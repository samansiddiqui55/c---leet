#include <iostream>
#include <vector> 
using namespace std ; 
void  num(vector <int>nums, int target){
  for (int i = 0 ; i <nums.size(); i++){
   if (nums[i]>=target) {
  cout <<i;
    break; 
  }
  }
}
int main(){
  vector<int >nums = {4,6,7,9,10};
  int target = 7; 
  num(nums,target); 
 }
// brute 

